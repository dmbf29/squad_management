<div class="container mt-3">
  <div class="row">
    <%= simple_form_for @squad, url: import_players_squad_path(@squad), method: :post do |f| %>
      <div class="col-12">
        <%= f.input :uploads, as: :file %>
      </div>
      <div class="col-12">
        <%= f.submit "Import", class: "btn btn-primary" %>
      </div>
    <% end %>
  </div>
</div>
